@model IEnumerable<TaskTracker.Models.TimerSetting>

@{
    ViewBag.Title = "TimerDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Timer Details</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTime)
        </th>
        <th>Description</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                Email Number:  @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditTimer", new { id = item.Id })

            </td>
        </tr>
    }

</table>
<div>
    @Html.ActionLink("Back to Dashboard", "AdminDashboard")
</div>

